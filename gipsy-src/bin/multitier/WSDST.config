##############################################################
## 
## Configuration properties to start up a WS DST instance.
## 
##############################################################

gipsy.GEE.multitier.wrapper.impl=gipsy.GEE.multitier.DST.ws.WSDSTWrapper
gipsy.GEE.multitier.DST.workingdir=../ws/

# Specify the port of the lookup service. If specified, its value
# must be the same as what is specified in reggie.config. Otherwise
# the default port 4160 will be used by Sun's Jini implementation.

gipsy.GEE.multitier.DST.jini.unicast.port=4162

# Specify the multicast group name of Jini services provided by DST.
# If specified, this value must be identical to the value specified
# in reggie.config, mahalo.config and outrigger.config

gipsy.GEE.TA.jini.discovery.multicast.group=gipsy

# Set the startup command line but without the configuration file passed to start.jar


# Specify the configuration file passed to start.jar. A thick configuration
# will start 4 services including httpd, reggie (lookup service),
# mahalo (transaction service) and outrigger (javaspace service).

gipsy.GEE.multitier.DST.jini.start.config.thick=startTransient4.config


# A thin configuration will start only javaspace and transaction services

gipsy.GEE.multitier.DST.jini.start.config.thin=startTransient2.config


# Specify the name of the configuration (and/or log) files of javaspace and trasaction
# implementation, i.e. outrigger and mahalo. The types of the file, i.e. .config or .log,
# will be automatically generated by JiniDSTManager, so here only the file names and paths
# shall be specified, and they must be consistent with what was specified in the top-level
# configuration files passed to start.jar. The content of the files will be generated by
# JiniDSTManager with both hard-coded and automatically generated content (such as port)

gipsy.GEE.multitier.DST.jini.javaspace.fileName=outrigger
gipsy.GEE.multitier.DST.jini.transaction.fileName=mahalo

# Specify if the DST supports transaction. If true, the TA using the TA configuration
# exported by this DST will use transaction.

gipsy.GEE.TA.jini.isTransactional=true

# Set the maximum number of instances allowed

gipsy.GEE.multitier.DST.maxInstances=1000

# Set if the DST is a public DST

gipsy.GEE.multitier.DST.isPublic=true

# Set the maximum number of active connections is allowed

gipsy.GEE.multitier.DST.maxActiveConnection=40000

